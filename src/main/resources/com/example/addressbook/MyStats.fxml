<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.addressbook.GUI.MyStats">

    <children>
        <!-- Your navigation bar -->
        <HBox alignment="CENTER">
            <!-- Add IDs to your buttons for easier access -->
            <Button id="myGoalsBtn" text="My Goals" onAction="#handleMyGoalsButtonClick"/>
            <Button id="myStatsBtn" text="My Stats" onAction="#handleMyStatsButtonClick"/>
            <Button id="myNotificationsBtn" text="My Notifications" onAction="#handleMyNotificationsButtonClick"/>
            <Button id="myHealthBtn" text="My Health" onAction="#handleMyHealthButtonClick"/>
            <Button id="myDataBtn" text="My Data" onAction="#handleMyDataButtonClick"/>
        </HBox>

        <!-- Your charts or other content -->
        <!-- For example, you can include a Pane or a SplitPane -->


    <HBox alignment="BOTTOM_CENTER">
        <Button fx:id="backButton" text="Back" onAction="#onBackButton"/>
        <Button fx:id="manualEntryButton" text="Manual Entry" onAction="#onManualEntryButton"/>
    </HBox>
    </children>

</VBox>


