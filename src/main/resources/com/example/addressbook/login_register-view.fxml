<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>

<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>


<?import javafx.scene.layout.HBox?>
<VBox alignment="TOP_LEFT" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.addressbook.GUI.LogInRegister">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <HBox alignment="BOTTOM_RIGHT" spacing="10.0">
        <Button fx:id="loginButton"
                text="LogIn"
                onAction="#onLogInButtonClick"
                disable="true"
        />
        <Button fx:id="signupButton"
                text="SignIn"
                onAction="#onSignUpButtonClick"
                disable="true"
        />
        <Button text="Button 1"/>
    </HBox>
</VBox>